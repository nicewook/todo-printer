  "bugs": {
    "url": "https://github.com/nicewook/todo-printer/issues"
  },
  "homepage": "https://github.com/nicewook/todo-printer#readme",
  "devDependencies": {
    "chokidar": "^4.0.3",
    "gray-matter": "^4.0.3",
    "marked": "^16.0.0"
  }
}
